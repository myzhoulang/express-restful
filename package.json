{
  "name": "express-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "build": "npx tsc && npm run sourcemap",
    "build:start": "npm run build && npm run sourcemap && npm run start",
    "build:local:start": "npm run build && npm run start",
    "sourcemap": "sentry-cli releases files $(git rev-parse --short HEAD) upload-sourcemaps --rewrite build",
    "start": "node ./build/index",
    "start:dev": "ts-node-dev ./src/index.ts",
    "dev": "nodemon",
    "test:unit": "mocha --recursive -r tsconfig-paths/register -r ts-node/register -r source-map-support/register src/**/*.spec.ts",
    "test:lint": "eslint --ext .ts ./src",
    "test:lint:fix": "npm run test:lint -- --fix",
    "test": "npm run test:lint && npm run test:unit"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run start:dev",
    "ext": "ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/cli": "^1.68.0",
    "@sentry/integrations": "^5.30.0",
    "@sentry/node": "^5.30.0",
    "@sentry/tracing": "^6.11.0",
    "@types/git-rev-sync": "^2.0.0",
    "@types/redis": "^2.8.31",
    "ali-oss": "^6.16.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.6.0",
    "exceljs": "^4.2.1",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "express-validator": "^6.12.1",
    "file-stream-rotator": "^0.5.7",
    "git-rev-sync": "^3.0.1",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^9.0.0",
    "lodash-es": "^4.17.21",
    "mongoose": "^6.1.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "nodemailer": "^6.6.3",
    "redis": "^3.1.2",
    "typescript": "^4.3.5",
    "xss": "^1.0.9"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.0.10",
    "@types/bcrypt": "^3.0.1",
    "@types/body-parser": "^1.19.1",
    "@types/chai": "^4.2.21",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/exceljs": "^1.3.0",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "0.0.42",
    "@types/helmet": "0.0.47",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mocha": "^7.0.2",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.0",
    "i": "^0.3.7",
    "mocha": "^9.1.4",
    "nodemon": "^2.0.12",
